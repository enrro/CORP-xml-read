    <DataSet Name="Company_DataSet">
      <Query>
        <DataSourceName>trnBPM</DataSourceName>
        <CommandText>SELECT 'ALL' AS CompanyCode,'ALL' AS CompanyDescription, 0 AS ORD UNION ALL
SELECT DISTINCT c.CompanyCode, c.CompanyCode + ' - ' + c.CompanyName AS CompanyDescription, 1 AS ORD
FROM            relSiteCompanyMepCode AS r INNER JOIN
                         catCompany AS c ON c.CompanyID = r.CompanyID
WHERE        c.Active = 1
ORDER BY 3,1</CommandText>
      </Query>
      <Fields>
        <Field Name="CompanyCode">
          <DataField>CompanyCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CompanyDescription">
          <DataField>CompanyDescription</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ORD">
          <DataField>ORD</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="MepCode_DataSet">
      <Query>
        <DataSourceName>trnBPM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyCOde">
            <Value>=Parameters!Company.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 'ALL' AS id, 'ALL' AS value,1 AS defaultValue, 0 AS IsParent, 0 AS BUID, 0 AS ORD UNION ALL
 SELECT    distinct 
  cat.MepCode AS id    
  ,cat.MepCode + ' - '+ cat.MepDesc  AS value    
  ,1 AS defaultValue   ,cat.IsParent , cat.BUID, 1 AS ORD
 FROM   relSiteCompanyMepCode rr        
                JOIN catCompany c ON c.CompanyID = rr.CompanyID        
                JOIN catMepCode cat ON cat.MepCodeID = rr.MepCodeID       
 WHERE cat.Active = 1    
and c.CompanyCode = @CompanyCOde
ORDER BY 6,1</CommandText>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="value">
          <DataField>value</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="defaultValue">
          <DataField>defaultValue</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="IsParent">
          <DataField>IsParent</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="BUID">
          <DataField>BUID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ORD">
          <DataField>ORD</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Site_DataSet">
      <Query>
        <DataSourceName>trnBPM</DataSourceName>
        <CommandText>SELECT 'ALL' AS ID, 'ALL' AS Value, 0 AS ORD UNION ALL
	SELECT DISTINCT s.SiteCode ID
		,s.SiteName AS Value, 1 AS ORD
	FROM dbo.catSite AS s WITH (NOLOCK)
	WHERE s.Active = 1
	ORDER BY 3,1</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Value">
