    <DataSet Name="Region_DataSet">
      <Query>
        <DataSourceName>trnBPM</DataSourceName>
        <CommandText>SELECT 'ALL' AS RegionCode, 'ALL' AS RegionName , 0 AS ORD
UNION ALL
SELECT distinct RegionCode, RegionName, 1 AS ORD FROM catRegion where Active = 1 and ParentRegionID IS null 
order by 3,1</CommandText>
      </Query>
      <Fields>
        <Field Name="RegionCode">
          <DataField>RegionCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RegionName">
          <DataField>RegionName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ORD">
          <DataField>ORD</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Company_DataSet">
      <Query>
        <DataSourceName>trnBPM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Region">
            <Value>=Parameters!Region.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 'ALL' AS CompanyCode,'ALL' AS CompanyDescription, 0 AS ORD
UNION ALL

SELECT DISTINCT c.CompanyCode, c.CompanyCode + ' - ' + c.CompanyName AS CompanyDescription, 1 AS ORD
FROM            catRegion AS cr INNER JOIN
                         catCountry AS cc ON cc.RegionID = cr.RegionID INNER JOIN
                         catSite AS s ON s.CountryID = cc.CountryID INNER JOIN
                         relSiteCompanyMepCode AS r ON r.SiteID = s.SiteID INNER JOIN
                         catCompany AS c ON c.CompanyID = r.CompanyID
WHERE        (cr.Active = 1) AND (cc.Active = 1) AND (s.Active = 1) AND (c.Active = 1) AND (cr.ParentRegionID IS NULL) AND (cr.RegionCode = @Region)
ORDER BY 3,1</CommandText>
      </Query>
      <Fields>
        <Field Name="CompanyCode">
          <DataField>CompanyCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CompanyDescription">
          <DataField>CompanyDescription</DataField>
          <rd:TypeName>System.String</rd:TypeName>
