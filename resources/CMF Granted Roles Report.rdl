    <DataSet Name="MepCode_DataSet">
      <Query>
        <DataSourceName>trnBPM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CompanyCode">
            <Value>=Parameters!Company.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 'ALL' AS id    , 'ALL'   AS value    ,
1 AS defaultValue   ,0 as IsParent,0 as BUID, 0 AS ORD
UNION ALL
 SELECT    distinct
  cat.MepCode AS id    
  ,cat.MepCode + ' - '+ cat.MepDesc  AS value    
  ,1 AS defaultValue   ,cat.IsParent , cat.BUID, 1 AS ORD
 FROM   relSiteCompanyMepCode rr        
                JOIN catCompany c ON c.CompanyID = rr.CompanyID        
                JOIN catMepCode cat ON cat.MepCodeID = rr.MepCodeID       
 WHERE cat.Active = 1    
and c.CompanyCode = @CompanyCode
Order by 3,1</CommandText>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="value">
          <DataField>value</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="defaultValue">
          <DataField>defaultValue</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="IsParent">
          <DataField>IsParent</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="BUID">
          <DataField>BUID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ORD">
          <DataField>ORD</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Segment_DataSet">
      <Query>
        <DataSourceName>trnBPM</DataSourceName>
        <CommandText>	SELECT  'ALL' AS ID
		,'ALL' AS Value, 0 AS ORD
UNION ALL
	SELECT DISTINCT s.SegmentCode ID
		,s.SegmentName AS Value, 1 AS ORD
	FROM dbo.catSegment AS s WITH (NOLOCK)
	JOIN dbo.catSegment AS s2 WITH (NOLOCK) ON s.ParentSegmentID = s2.SegmentID
	WHERE s.Active = 1
		AND s.ParentSegmentID IS NOT NULL
	ORDER BY 3,1</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Value">
          <DataField>Value</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ORD">
          <DataField>ORD</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="PaymentTerms_DataSet">
      <Query>
        <DataSourceName>trnBPM</DataSourceName>
        <CommandText>SELECT 'ALL' AS ID, 'ALL' AS Value, 0 AS ORD
UNION ALL
SELECT     distinct 
 cb.code as ID,  
 ISNULL(cb.description, cb.code) AS Value      , 1 AS ORD
 FROM catBAANMiscellany cb      
 WHERE 
 cb.type = 'Payment Term'
 AND cb.Active = 1    
 ORDER BY 3,2</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
