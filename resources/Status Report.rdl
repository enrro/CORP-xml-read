    <DataSet Name="Region_DataSet">
      <Query>
        <DataSourceName>trnBPM</DataSourceName>
        <CommandText>SELECT   '&lt;ALL&gt;' AS RegionCode,'&lt;ALL&gt;'  AS RegionName 
UNION
SELECT RegionCode, RegionName FROM catRegion where Active = 1 and ParentRegionID IS null </CommandText>
      </Query>
      <Fields>
        <Field Name="RegionCode">
          <DataField>RegionCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RegionName">
          <DataField>RegionName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Company_DataSet">
      <Query>
        <DataSourceName>trnBPM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Region">
            <Value>=Parameters!Region.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT   '&lt;ALL&gt;' AS CompanyCode ,'&lt;ALL&gt;' AS CompanyDescription
UNION
SELECT DISTINCT c.CompanyCode, c.CompanyCode + ' - ' + c.CompanyName AS CompanyDescription
FROM            catRegion AS cr INNER JOIN
                         catCountry AS cc ON cc.RegionID = cr.RegionID INNER JOIN
                         catSite AS s ON s.CountryID = cc.CountryID INNER JOIN
                         relSiteCompanyMepCode AS r ON r.SiteID = s.SiteID INNER JOIN
                         catCompany AS c ON c.CompanyID = r.CompanyID
WHERE        (cr.Active = 1) AND (cc.Active = 1) AND (s.Active = 1) AND (c.Active = 1) AND (cr.ParentRegionID IS NULL) AND (cr.RegionCode = @Region)
ORDER BY 1</CommandText>
      </Query>
      <Fields>
        <Field Name="CompanyCode">
          <DataField>CompanyCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CompanyDescription">
          <DataField>CompanyDescription</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="MepCode_DataSet">
      <Query>
        <DataSourceName>trnBPM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Company">
            <Value>=Parameters!Company.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT '&lt;ALL&gt;' AS id,'&lt;ALL&gt;' AS value
UNION
 SELECT DISTINCT 
		cat.MepCode AS id      
		,cat.MepCode + ' - '+ cat.MepDesc  AS value         
	 FROM   relSiteCompanyMepCode rr          
	 JOIN catCompany c ON c.CompanyID = rr.CompanyID          
	 JOIN catMepCode cat ON cat.MepCodeID = rr.MepCodeID         
	 WHERE cat.Active = 1 AND (c.CompanyCode = @Company OR c.CompanyCode = @Company) </CommandText>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="value">
          <DataField>value</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
