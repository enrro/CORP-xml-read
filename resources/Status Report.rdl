    <DataSet Name="Status_DataSet">
      <Query>
        <DataSourceName>trnBPM</DataSourceName>
        <CommandText>/*SELECT   '&lt;ALL&gt;' AS  [Status] 
UNION
SELECT DISTINCT [Status] FROM trnPayload with(NOLOCK) where PluginID IN (SELECT PluginID from catPlugin where PluginName = 'CMF')*/
SELECT   '&lt;ALL&gt;' AS  [Status]  UNION
SELECT 'Saved but not Submitted' UNION
SELECT 'In Process - Finance Controller' UNION
SELECT 'In Process - Credit Team' UNION
SELECT 'In Process - CMF Team' UNION
SELECT 'In Process - Segment VP' UNION
SELECT 'Unsubmitted' UNION
SELECT 'Completed' UNION
SELECT 'Rejected' UNION
SELECT 'Cancelled'</CommandText>
      </Query>
