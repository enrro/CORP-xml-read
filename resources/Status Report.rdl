  <DataSets>
    <DataSet Name="CMF_rptStatusReport">
      <Query>
        <DataSourceName>trnBPM</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@InitDate">
            <Value>=Format(Parameters!InitDate.Value, "MM/dd/yyyy")</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Format(Parameters!EndDate.Value, "MM/dd/yyyy")</Value>
          </QueryParameter>
          <QueryParameter Name="@Region">
            <Value>=IIF(Parameters!Region.Value.ToString().Equals("&lt;ALL&gt;"),"",Parameters!Region.Value)</Value>
          </QueryParameter>
          <QueryParameter Name="@Company">
            <Value>=IIF(Parameters!Company.Value.ToString().Equals("&lt;ALL&gt;"),"",Parameters!Company.Value)</Value>
          </QueryParameter>
          <QueryParameter Name="@MepCode">
            <Value>=IIF(Parameters!MepCode.Value.ToString().Equals("&lt;ALL&gt;"),"",Parameters!MepCode.Value)</Value>
          </QueryParameter>
          <QueryParameter Name="@Customer2">
            <Value>=IIF(Parameters!Customer2.Value.ToString().Equals("&lt;ALL&gt;"),"",Parameters!Customer2.Value)</Value>
          </QueryParameter>
          <QueryParameter Name="@Status">
            <Value>=Switch(
	Parameters!Status.Value.ToString().Equals("&lt;ALL&gt;"),""
	,Parameters!Status.Value.ToString().Contains("In Process"), "In Process"
	,True, Parameters!Status.Value.ToString()
)</Value>
          </QueryParameter>
          <QueryParameter Name="@RequestType">
            <Value>=IIF(Parameters!RequestType.Value.ToString().Equals("&lt;ALL&gt;"),"",Parameters!RequestType.Value)</Value>
          </QueryParameter>
          <QueryParameter Name="@Approver">
            <Value>=Parameters!Approver.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Activity">
            <Value>=Switch(
	Parameters!Status.Value.ToString().Contains("Finance Controller"), "Finance Controller"
	,Parameters!Status.Value.ToString().Contains("Credit Team"), "Credit Team"
	,Parameters!Status.Value.ToString().Contains("CMF Team"), "CMF Team"
	,Parameters!Status.Value.ToString().Contains("Segment VP"), "Segment VP"
	,True, ""
)</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandType>StoredProcedure</CommandType>
        <CommandText>CMF_rptStatusReport</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
